libxso_target=	$(libxso_base)-$(SOVERSION).la
libxso_default=	$(libxso_base).@SOSUFFIX@
libxso_major=	$(libxso_base)-$(SOMAJOR).@SOSUFFIX@

##################################################
# Java API.
#
# Java support is optional and requires shared librarires.
##################################################
