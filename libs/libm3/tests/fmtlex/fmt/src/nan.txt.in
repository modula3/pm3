# This file tests formating of the floating-point value Nan for each of the 3
# formatting styles. 

# Sci -------------------------------------------------------------------------

r S 0 NaN	# " NaN  ",        " 0.0/0.0"
r S 1 NaN	# " NaN    ",      " 0.0/0.0"
r S 2 NaN	# " NaN     ",     " 0.0/0.0 "
l S 0 NaN	# " NaN   ",       " 0.0d0/0.0d0"
l S 1 NaN	# " NaN     ",     " 0.0d0/0.0d0"
l S 4 NaN	# " NaN        ",  " 0.0d0/0.0d0"
l S 5 NaN	# " NaN         ", " 0.0d0/0.0d0 "
e S 0 NaN	# " NaN   ",       " 0.0x0/0.0x0"
e S 1 NaN	# " NaN     ",     " 0.0x0/0.0x0"
e S 4 NaN	# " NaN        ",  " 0.0x0/0.0x0"
e S 5 NaN	# " NaN         ", " 0.0x0/0.0x0 "

# Fix -------------------------------------------------------------------------

r F 0 NaN	# "NaN", "0.0/0.0"
r F 5 NaN	# "NaN", "0.0/0.0"
l F 0 NaN	# "NaN", "0.0d0/0.0d0"
l F 5 NaN	# "NaN", "0.0d0/0.0d0"
e F 0 NaN	# "NaN", "0.0x0/0.0x0"
e F 5 NaN	# "NaN", "0.0x0/0.0x0"

# Auto ------------------------------------------------------------------------

r A 0 NaN	# "NaN", "0.0/0.0"
r A 5 NaN	# "NaN", "0.0/0.0"
l A 0 NaN	# "NaN", "0.0d0/0.0d0"
l A 5 NaN	# "NaN", "0.0d0/0.0d0"
e A 0 NaN	# "NaN", "0.0x0/0.0x0"
e A 5 NaN	# "NaN", "0.0x0/0.0x0"
