if defined ("_all")
  if   stale ("y.tab.o", "../src/flex-bison/y.tab.c")
    or stale ("y.tab.o", "../src/flex-bison/lex.yy.c")
    or stale ("y.tab.o", "../src/hash.h")
    or stale ("y.tab.o", "../src/lex_help.h")
    delete_file ("y.tab.o")
  end
end

m3_option  ("-X1@-I../src/flex-bison@") %-- so the C compiler can find lex.yy.c
import_lib ("fl", "/usr/lib")           %-- That's where the flex stuff lives
c_source   ("y.tab")



