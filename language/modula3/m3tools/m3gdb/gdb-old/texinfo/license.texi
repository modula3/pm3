\input texinfo
@setfilename license.info

@ifinfo
@format
START-INFO-DIR-ENTRY
* COPYING: (license).		The GNU General Public License
END-INFO-DIR-ENTRY
@end format
@end ifinfo

@settitle GPL
@iftex
@headings doubleafter
@setchapternewpage off
@end iftex
@finalout

@node Top, , (dir), (dir)
@unnumbered GNU GENERAL PUBLIC LICENSE

@include gpl.texinfo

@bye
