(VBox 
  (Macro TB BOA (text)
    `(Shape (Width 60) (Text RightAlign ,text)))
  (Glue 10)
  (HBox (TB "N: ") (Numeric (Min 1) (Max 1000) %N =50) Fill)
  (Glue 10)
  (HBox (TB "k: ") (Numeric (Min 1) (Max 1000) %k =5) Fill)
  (Glue 10))
