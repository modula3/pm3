if SYSTEM_LIBS contains "X11"

  import("libm3")
  import_sys_lib ("X11")

  if equal(TARGET, "ALPHA_OSF") or equal(TARGET, "Tru64v5")
    include_dir ("ALPHA_OSF")
  else
    include_dir ("Vanilla")
  end
  include_dir ("Common")
  Library ("m3X11R4")

end
