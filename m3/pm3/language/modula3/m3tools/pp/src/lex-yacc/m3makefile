if defined ("_all")
  if   stale ("y.tab.o", "../src/lex-yacc/y.tab.c")
    or stale ("y.tab.o", "../src/lex-yacc/lex.yy.c")
    or stale ("y.tab.o", "../src/hash.h")
    or stale ("y.tab.o", "../src/lex_help.h")
    delete_file ("y.tab.o")
  end
end

import_lib ("l", "/usr/lib")            %-- That's where the lex stuff lives
h_source   ("lex.yy")                   % ugly but it works
c_source   ("y.tab")



