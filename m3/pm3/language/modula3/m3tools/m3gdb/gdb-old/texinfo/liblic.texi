\input texinfo
@setfilename lgpl.info

@ifinfo
@format
START-INFO-DIR-ENTRY
* COPYING LIBS: (lgpl).		The GNU Library General Public License
END-INFO-DIR-ENTRY
@end format
@end ifinfo

@settitle LGPL
@iftex
@headings doubleafter
@setchapternewpage off
@end iftex
@finalout

@clear lgpl-appendix
@node Top, , (dir), (dir)
@include lgpl.texinfo

@bye
