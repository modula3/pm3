Interface("TCPPosix")
interface("Herrno")
implementation("IP")
implementation("TCP")
interface("TCPHack")

if equal (TARGET, "ALPHA_OSF") or equal(TARGET, "DS3100")
   implementation("TCPHack")
else
   implementation("TCPHackNull")
end


