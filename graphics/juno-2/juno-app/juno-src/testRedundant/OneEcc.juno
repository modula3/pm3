PROC Cmd0() IS 
  IF 
    VAR 
      a = (136.3, 371.7), b = (234.5, 371.7), ecc = 0.75, 
      mid = Geometry.Mid(a, e), d ~ (193.46127, 311.81345), 
      e = (218.1, 286) 
    IN 
      Ellipse.OnEcc(d, a, b, ecc) AND 
      Geometry.Colinear(a, d, e) AND 
      a HOR b -> 
        Ellipse.DrawEcc(a, b, ecc); 
        PS.MoveTo(a); 
        PS.LineTo(d); 
        PS.Type(a, Unparse.Value(mid)); 
        PS.MoveTo(a); 
        PS.LineTo(e) 
    END 
  FI 
END;
